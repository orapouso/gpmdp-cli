{
  "name": "gpmdp-cli",
  "version": "1.0.0",
  "description": "Console cli for Google Play Music Desktop Player",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava ./test/*.js --verbose",
    "report": "nyc report --reporter=lcov",
    "check": "nyc check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orapouso/gpmdp-cli.git"
  },
  "keywords": [
    "gdmdp",
    "google-play-music-desktop-player"
  ],
  "author": "Rafael Raposo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orapouso/gpmdp-cli/issues"
  },
  "homepage": "https://github.com/orapouso/gpmdp-cli#readme",
  "dependencies": {
    "bluebird": "^3.4.6",
    "debug": "^2.2.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "eslint": "^3.5.0",
    "eslint-plugin-ava": "^3.0.0",
    "nyc": "^8.3.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6"
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": ["**/node_modules/**"],
    "reporter": ["lcov"],
    "cache": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
